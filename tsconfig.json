{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts"
, "app/product/[id]/page.jsx", "app/seller/layout.jsx", "app/seller/page.jsx", "app/layout.jsx", "app/add-address/page.jsx", "app/seller/orders/page.jsx", "app/seller/product-list/page.jsx", "components/CTABanner.jsx", "components/FeaturedProduct.jsx", "components/Footer.jsx", "components/Loading.jsx", "components/MainLayout.jsx", "components/Navbar.jsx", "components/Summary.jsx", "components/Slider.jsx", "components/Products.jsx", "components/ProductCard.jsx", "components/seller/Navbar.jsx", "components/seller/Sidebar.jsx"  ],
  "exclude": ["node_modules"]
}
